{
    "version": 2,
    "builds": [
        {
            "src": "src/server.js",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "src/server.js"
        },
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ]
}
